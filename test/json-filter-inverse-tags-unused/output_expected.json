      "bad-request-response": {
        "type": "object",
        "properties": {
          "issues": {
            "type": "array",
            "description": "List of issues",
            "items": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "description": "Type of issue"
                },
                "code": {
                  "type": "string",
                  "description": "Code of issue"
                },
                "message": {
                  "type": "string",
                  "description": "Message of issue"
                }
              }
            }
          },
          "reference": {
            "type": "string",
            "description": "Reference of the issue"
          }
        }
      },
      "component-create": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Human readable identification for this record",
            "pattern": "^\\S.*$",
            "example": "CEMENT1"
          },
          "name": {
            "type": "string",
            "description": "Name of item",
            "example": "CEMENT1"
          },
          "locations": {
            "description": "List of locations GUIDs where component is available. all: true means available at all locations",
            "type": "object",
            "additionalProperties": {
              "type": "boolean"
            },
            "example": {
              "all": true
            },
            "x-propertyNames": {
              "minLength": 1
            }
          },
          "costs": {
            "type": "array",
            "description": "Array of component/location associated costs.",
            "items": {
              "type": "object",
              "description": "Cost of component at the associated location.",
              "properties": {
                "batchCode": {
                  "type": "string",
                  "description": "Batch code used for Batching.",
                  "example": "CEMENT",
                  "nullable": true
                },
                "locationRef": {
                  "type": "string",
                  "description": "GUID for the associated location.",
                  "example": "1fd060d-8b61-5d58-975b-975b975b975b"
                },
                "price": {
                  "type": "object",
                  "description": "Cost properties of component at the associated location",
                  "properties": {
                    "value": {
                      "type": "number",
                      "description": "Cost of the component",
                      "example": 192.83
                    },
                    "currencyCode": {
                      "$ref": "#/components/schemas/currency-codes",
                      "description": "ISO 4217 currency code e.g. USD, <a href=\"http://developer.commandalkon.io/docs/common-currency-codes\">Other Codes</a>",
                      "example": "USD"
                    },
                    "uomCode": {
                      "$ref": "#/components/schemas/UOM",
                      "description": "ISO UOM for the associated pricing. <a href=\"http://developer.commandalkon.io/docs/common-uoms\">Common UOMs</a>"
                    }
                  },
                  "required": [
                    "currencyCode",
                    "uomCode",
                    "value"
                  ],
                  "additionalProperties": false
                }
              },
              "required": [
                "locationRef",
                "price"
              ],
              "additionalProperties": false
            }
          },
          "batchCode": {
            "type": "string",
            "description": "Batch code used for Batching."
          },
          "batchUomCode": {
            "$ref": "#/components/schemas/UOM",
            "description": "ISO UOM used for batching calculations. <a href=\"http://developer.commandalkon.io/docs/common-uoms\">Common UOMs</a>",
            "example": "LBR"
          },
          "productRef": {
            "type": "string",
            "description": "Product Reference GUID associated with this component, used for Product Inventory settings"
          },
          "uomCode": {
            "$ref": "#/components/schemas/UOM",
            "description": "ISO UOM used for inventory calculations. <a href=\"http://developer.commandalkon.io/docs/common-uoms\">Common UOMs</a>",
            "example": "STN"
          },
          "weight": {
            "type": "number",
            "description": "Weight relationship between the batch UOM and the inventory UOM, when they are different.",
            "example": "0.0005"
          },
          "materialType": {
            "type": "string",
            "description": "Material type of the component, used for QC and Batch",
            "enum": [
              "WATER",
              "CEMENT",
              "FLYASH",
              "SLAG",
              "SILICAFUME",
              "SAND",
              "COARSEAGG",
              "ADMIX",
              "ADDITIVE",
              "OTHER"
            ],
            "example": "CEMENT"
          },
          "status": {
            "type": "string",
            "description": "Status of the item",
            "enum": [
              "ACTIVE",
              "INACTIVE"
            ],
            "default": "ACTIVE",
            "example": "ACTIVE"
          }
        },
        "required": [
          "id",
          "name"
        ],
        "additionalProperties": false
      },
