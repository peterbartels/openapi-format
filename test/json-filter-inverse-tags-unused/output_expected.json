          },
          "batchCode": {
            "type": "string",
            "description": "Batch code used for Batching."
          },
          "productRef": {
            "type": "string",
            "description": "Product Reference GUID associated with this component, used for Product Inventory settings"
          },
          "batchUomCode": {
            "$ref": "#/components/schemas/UOM",
            "description": "ISO UOM used for batching calculations. <a href=\"http://developer.commandalkon.io/docs/common-uoms\">Common UOMs</a>"
          },
          "uomCode": {
